  <row id="Lookups_Panel5">
    <panel id="panel_accordion_togglepanel_false_lookups_222" depends="$gadget_lib_loaded$">
      <title>4. Lookup Blacklisting</title>
      <table>
        <search>
          <query>| from lookup:splunk_rest_configs_conf_files_all_kv_store_lookup 
| fields Splunk_Instance config_file app sharing author stanza title updated 
| search config_file="distsearch.conf" stanza="replicationBlacklist" 
| `get_shcluster_label(Splunk_Instance)` 
| makemv delim="###" title 
| mvexpand title 
| search title!="\[*" 
| rex field=title "(?&lt;Property&gt;^\w+)\s*=\s*(?&lt;Value&gt;.*?)$" 
| rex field=Value mode=sed "s/\"//g" 
| stats 
    Values(*) As *
    Dc(Value) As DC_Value
    by shcluster_label app config_file stanza Property 
| table shcluster_label config_file author app sharing stanza Property Value DC_Value 
| where Property!="updated" 
| table shcluster_label config_file app stanza  Property Value
| search shcluster_label="$shcluster_label3$" AND app="$app3$"</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
        </search>
        <option name="count">5</option>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
        <option name="rowNumbers">true</option>
        <option name="wrap">false</option>
      </table>
    </panel>
  </row>
