<form theme="dark" script="tabs.js, load.js" stylesheet="tabs.css, load.css" onunloadCancelJobs="true">
  <label>SH - 03 - Lookups Deep Analysis - Dashboard - v1</label>
  <description>Splunk Lookups Analysis Dashboard</description>
  <search id="splunk_sh_03_lookups_deep_analysis_dashboard_v1">
    <query>| from lookup:splunk_rest_admin_transforms_lookup_sh_kv_store_lookup 
| fields _time shcluster_label Splunk_Instance app Lookup author sharing Filename Type Collection updated disabled Match_Type Replicate Allow_Caching Filter Index_Fields_List Min_Matches Max_Matches Fields_List case_sensitive_match acl_perms_read acl_perms_write file_size acl_removable
| join shcluster_label app sharing Filename type=left 
    [| from lookup:splunk_rest_admin_lookup_table_files_sh_kv_store_lookup 
    | fields shcluster_label,Splunk_Instance,app,sharing,author,disabled,updated,Filename,Lookup_File_Path acl_removable]
| `get_instance_roles(Splunk_Instance)`
| `get_identity_info(shcluster_label,author)` | `get_identity_info(author)` | `fillnull_identity_info`
| `strftime_format(updated)`
| eval Type = if(match(Lookup_File,".csv"), "file", Type) , if(isnull(Lookup_File), "file", Type) , Lookup = if(isnull(Lookup), "NO-DEFINITION", Lookup)
| eval file_size=file_size/1024/1024
| fillnull value="0" Min_Matches disabled file_size
| fillnull value="100" Max_Matches 
| fillnull value="true" Allow_Caching Replicate case_sensitive_match
| fillnull value="EXACT" Match_Type
| fillnull value="Undefined" Splunk_Instance Splunk_Roles app author sharing Lookup Filename Type Collection Lookup_File_Path Fields_List
| search shcluster_label="$shcluster_label3$" Splunk_Roles="$Splunk_Roles3$" Splunk_Instance="$Splunk_Instance3$" app="$app3$" Lookup="$Lookup$" author="$author3$" sharing="$sharing3$" Filename="$Filename$" Lookup_File_Path="$Lookup_File_Path$" Collection="$Collection$" Match_Type="$Match_Type$" Type="$Type3$" case_sensitive_match="$case_sensitive_match$" emp_name="$emp_name$" emp_title="$emp_title$" emp_type="$emp_type$" emp_status="$emp_status$" emp_dep="$emp_dep$" emp_lob1="$emp_lob1$" emp_cc="$emp_cc$" emp_city="$emp_city$" emp_region1="$emp_region1$" emp_country="$emp_country$" Fields_List="$Fields_List$" file_size &gt;= $file_size$ Replicate="$Replicate$" acl_removable=$acl_removable$</query>
  </search>
  <search id="splunk_sh_06_knowledge_objects_analysis_auto_lookups">
    <query>| from lookup:splunk_rest_data_props_lookups_sh_kv_store_lookup 
| fields _time shcluster_label Splunk_Instance Splunk_Roles app sharing stanza attribute transform overwrite value author updated acl_perms_read acl_perms_write acl_removable
| `get_instance_roles(Splunk_Instance)`
| `get_identity_info(shcluster_label,author)` | `get_identity_info(author)` | `fillnull_identity_info`
| `strftime_format(updated)`
| rex field=attribute mode=sed "s/LOOKUP-//g"
| lookup splunk_rest_admin_transforms_lookup_sh_kv_store_lookup shcluster_label app Lookup As transform OUTPUT Filename Type Fields_List Collection
| fillnull value="Undefined" Splunk_Instance Splunk_Roles app sharing stanza attribute transform overwrite value author
| search shcluster_label="$shcluster_label4$" Splunk_Instance="$Splunk_Instance4$" Splunk_Roles="$Splunk_Roles4$" app="$app4$" author="$author4$" attribute="$attribute$" stanza="$stanza$" emp_name="$emp_name$" emp_title="$emp_title$" emp_lob1="$emp_lob1$" emp_dep="$emp_dep$" emp_type="$emp_type$" emp_status="$emp_status$" sharing="$sharing4$" overwrite="$overwrite$" transform="$transform$" acl_removable=$acl_removable$</query>
  </search>
  <fieldset submitButton="false" autoRun="false"></fieldset>
