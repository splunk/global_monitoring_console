  <label>SH - 07 - Fields - Dashboard - v1.5.90</label>
  <search id="splunk_sh_07_fields_dashboard_calculated_fields">
    <query>
  | from lookup:splunk_rest_data_props_calcfields_sh_kv_store_lookup 
  | fields shcluster_label Splunk_Instance app sharing stanza name value author updated acl_perms_read acl_perms_write 
  | `get_instance_roles(Splunk_Instance)`
  |	`get_identity_info_simple(shcluster_label,author)`
  | `strftime_format(updated)`
  | fillnull value="Undefined" Splunk_Instance Splunk_Roles app sharing stanza name value author
  | search shcluster_label="$shcluster_label$" Splunk_Instance="*$Splunk_Instance$*" Splunk_Roles="*$Splunk_Roles$*" app="$app$" author="$author$" name="*$name$*" stanza="$stanza$" emp_name="$emp_name$" emp_ttl="$emp_ttl$" emp_lob="$emp_lob$" emp_dep="$emp_dep$" emp_type="$emp_type$" emp_status="$emp_status$" sharing="$sharing$"</query>
  </search>
  <search id="splunk_sh_07_fields_dashboard_field_extractions">
    <query>
| from lookup:splunk_rest_data_props_extractions_sh_kv_store_lookup 
| fields shcluster_label Splunk_Instance app sharing stanza attribute value author type updated acl_perms_read acl_perms_write 
| `get_instance_roles(Splunk_Instance)`
|	`get_identity_info_simple(shcluster_label,author)`
| `strftime_format(updated)`
| lookup local=True splunk_rest_data_transforms_extractions_sh_kv_store_lookup Splunk_Instance app title as value OUTPUTNEW Type FORMAT REGEX DELIMS CLEAN_KEYS DEST_KEY FIELDS MV_ADD REPEAT_MATCH SOURCE_KEY WRITE_META
| eval Transform_Type = case(match(attribute, "^REPORT"), "REPORT", match(attribute, "^EXTRACT"), "EXTRACT", true(), attribute)
| rex field=attribute mode=sed "s/^REPORT-|^EXTRACT-//g"
| fillnull value="Undefined" Splunk_Instance app sharing stanza attribute value author type Type FORMAT REGEX DELIMS CLEAN_KEYS DEST_KEY FIELDS MV_ADD REPEAT_MATCH SOURCE_KEY WRITE_META
| search shcluster_label="$shcluster_label$" Splunk_Instance="*$Splunk_Instance$*" Splunk_Roles="*$Splunk_Roles$*" app="$app$" author="$author$" attribute="*$attribute$*" type="$type$" stanza="$stanza$" emp_name="$emp_name$" emp_ttl="$emp_ttl$" emp_lob="$emp_lob$" emp_dep="$emp_dep$" emp_type="$emp_type$" emp_status="$emp_status$" sharing="$sharing$" Transform_Type="$Transform_Type$"
</query>
  </search>
  <search id="splunk_sh_07_fields_dashboard_field_aliases">
    <query>
| from lookup:splunk_rest_data_props_fieldaliases_sh_kv_store_lookup 
| fields shcluster_label Splunk_Instance app author stanza attribute value sharing updated acl_perms_read acl_perms_write 
| `get_instance_roles(Splunk_Instance)`
|	`get_identity_info_simple(shcluster_label,author)` 
| `strftime_format(updated)`
| rex field=attribute mode=sed "s/FIELDALIAS-//g"
| fillnull value="Undefined" Splunk_Instance Splunk_Roles app author stanza attribute value sharing
| search shcluster_label="$shcluster_label$" Splunk_Instance="*$Splunk_Instance$*" Splunk_Roles="*$Splunk_Roles$*" app="$app$" author="$author$" attribute="*$attribute$*" stanza="$stanza$" emp_name="$emp_name$" emp_ttl="$emp_ttl$" emp_lob="$emp_lob$" emp_dep="$emp_dep$" emp_type="$emp_type$" emp_status="$emp_status$" sharing="$sharing$"
</query>
